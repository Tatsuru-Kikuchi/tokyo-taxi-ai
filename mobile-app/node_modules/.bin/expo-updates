#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tatsurukikuchi/development/tokyo-taxi-ai/mobile-app/node_modules/.pnpm/expo-updates@0.28.17_expo@54.0.10_@babel+core@7.28.4_@expo+metro-runtime@5.0.5_react-na_4b27775e48249726a0316ac1bced5447/node_modules/expo-updates/bin/node_modules:/Users/tatsurukikuchi/development/tokyo-taxi-ai/mobile-app/node_modules/.pnpm/expo-updates@0.28.17_expo@54.0.10_@babel+core@7.28.4_@expo+metro-runtime@5.0.5_react-na_4b27775e48249726a0316ac1bced5447/node_modules/expo-updates/node_modules:/Users/tatsurukikuchi/development/tokyo-taxi-ai/mobile-app/node_modules/.pnpm/expo-updates@0.28.17_expo@54.0.10_@babel+core@7.28.4_@expo+metro-runtime@5.0.5_react-na_4b27775e48249726a0316ac1bced5447/node_modules:/Users/tatsurukikuchi/development/tokyo-taxi-ai/mobile-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tatsurukikuchi/development/tokyo-taxi-ai/mobile-app/node_modules/.pnpm/expo-updates@0.28.17_expo@54.0.10_@babel+core@7.28.4_@expo+metro-runtime@5.0.5_react-na_4b27775e48249726a0316ac1bced5447/node_modules/expo-updates/bin/node_modules:/Users/tatsurukikuchi/development/tokyo-taxi-ai/mobile-app/node_modules/.pnpm/expo-updates@0.28.17_expo@54.0.10_@babel+core@7.28.4_@expo+metro-runtime@5.0.5_react-na_4b27775e48249726a0316ac1bced5447/node_modules/expo-updates/node_modules:/Users/tatsurukikuchi/development/tokyo-taxi-ai/mobile-app/node_modules/.pnpm/expo-updates@0.28.17_expo@54.0.10_@babel+core@7.28.4_@expo+metro-runtime@5.0.5_react-na_4b27775e48249726a0316ac1bced5447/node_modules:/Users/tatsurukikuchi/development/tokyo-taxi-ai/mobile-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../expo-updates/bin/cli.js" "$@"
else
  exec node  "$basedir/../expo-updates/bin/cli.js" "$@"
fi
